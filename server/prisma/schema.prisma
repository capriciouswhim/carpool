// Try Prisma Accelerate: https://pris.ly/cli/accelerate-init

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator tsed {
  provider = "tsed-prisma"
}

model history {
  pool_number Int
  lane_date   String
  lane_time   String
  call_time   String?
  send_time   String?
  exit_time   String?

  @@id([pool_number, lane_date])
}

model flag {
  name  String  @id
  value Boolean
}
